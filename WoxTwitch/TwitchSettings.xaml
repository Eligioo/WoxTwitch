<UserControl x:Class="WoxTwitch.TwitchSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WoxTwitch"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Loaded="OnLoaded">
    <Grid>
        <RadioButton x:Name="radioBrowser" Content="Browser" HorizontalAlignment="Left" Margin="50,95,0,0" VerticalAlignment="Top" GroupName="Launch"/>
        <RadioButton x:Name="radioLivestreamer" Content="Livestreamer" HorizontalAlignment="Left" Margin="50,114,0,0" VerticalAlignment="Top" GroupName="Launch"/>
    </Grid>
</UserControl>
