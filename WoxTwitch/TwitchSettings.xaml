<UserControl x:Class="WoxTwitch.TwitchSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WoxTwitch"
             mc:Ignorable="d" 
             d:DesignHeight="300" Loaded="OnLoaded" Width="757.091">
    <Grid>
        <RadioButton x:Name="radioBrowser" Content="Browser" HorizontalAlignment="Left" Margin="10,14,0,0" VerticalAlignment="Top" GroupName="Launch" Checked="RadioBrowserEvent"/>
        <RadioButton x:Name="radioLivestreamer" Content="Livestreamer" HorizontalAlignment="Left" Margin="10,33,0,0" VerticalAlignment="Top" GroupName="Launch" Checked="RadioLivestreamerEvent"/>
        <TextBox x:Name="textLivestreamer" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="448" Margin="10,53,0,0" GotFocus="textBox_GotFocus"/>
    </Grid>
</UserControl>
